<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <ROOT>
        <type>array</type>
        <el>
            <settings.baseUrl>
                <TCEforms>
                    <label>LLL:EXT:proxy/Resources/Private/Language/locallang.xlf:baseUrl</label>
                    <config>
                        <type>input</type>
                    </config>
                </TCEforms>
            </settings.baseUrl>
            <settings.startUrl>
                <TCEforms>
                    <label>LLL:EXT:proxy/Resources/Private/Language/locallang.xlf:url</label>
                    <config>
                        <type>input</type>
                    </config>
                </TCEforms>
            </settings.startUrl>
            <settings.pathPrefix>
                <TCEforms>
                    <label>Path prefix</label>
                    <config>
                        <type>input</type>
                    </config>
                </TCEforms>
            </settings.pathPrefix>
            <settings.pageTitleSuffix>
                <TCEforms>
                    <label>Page title suffix</label>
                    <config>
                        <type>input</type>
                    </config>
                </TCEforms>
            </settings.pageTitleSuffix>
            <settings.js.whitelist>
                <TCEforms>
                    <label>JavaScript whitelist</label>
                    <config>
                        <type>text</type>
                    </config>
                </TCEforms>
            </settings.js.whitelist>
            <settings.css.whitelist>
                <TCEforms>
                    <label>CSS whitelist</label>
                    <config>
                        <type>text</type>
                    </config>
                </TCEforms>
            </settings.css.whitelist>
            <settings.plugins>
                <TCEforms>
                    <label>Plugins</label>
                    <config>
                        <type>select</type>
                        <renderType>selectMultipleSideBySide</renderType>
                        <itemsProcFunc>WapplerSystems\Proxy\Backend\Form\PluginsRegistry->getPluginsList</itemsProcFunc>
                    </config>
                </TCEforms>
            </settings.plugins>
        </el>
    </ROOT>
</T3DataStructure>
