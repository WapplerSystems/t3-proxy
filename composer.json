{
	"name": "wapplersystems/proxy",
	"type": "typo3-cms-extension",
	"description": "TYPO3 proxy extension for showing content of another system inside TYPO3",
	"autoload": {
		"psr-4": {
			"WapplerSystems\\Proxy\\": "Classes"
		}
	},
	"keywords": [
		"TYPO3 CMS"
	],
	"authors": [
		{
			"name": "Sven Wappler",
			"email": "info@wappler.systems",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^11",
		"wapplersystems/php-html-parser": "^1",
		"ext-curl": "*"
	},
	"replace": {
	},
	"license": "GPL-2.0+",
	"extra": {
		"typo3/cms": {
			"extension-key": "proxy"
		}
	}
}
